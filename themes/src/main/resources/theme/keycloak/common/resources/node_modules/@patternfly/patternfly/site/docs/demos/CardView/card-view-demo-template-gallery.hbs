    {{/card}}
    {{#> card card--id="card-4" card--modifier="pf-m-hoverable pf-m-compact"}}
      {{#> card-header}}
        <img src="/assets/images/camel-avro_200x150.png" width="60px" alt="Logo">
        {{#> card-actions}}
