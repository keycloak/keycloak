<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <parent>
        <artifactId>keycloak-parent</artifactId>
        <groupId>org.keycloak</groupId>
        <version>15.0.0-SNAPSHOT</version>
        <relativePath>../../../pom.xml</relativePath>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>keycloak-fuse7-integration-pom</artifactId>
    <packaging>pom</packaging>
    <name>Keycloak Fuse 7.0 Integration</name>

    <properties>
        <fuse7.version>7.0.1-SNAPSHOT</fuse7.version>
        <jetty9.version>9.4.2.v20170220</jetty9.version>
    </properties>

    <modules>
        <module>camel-undertow</module>
        <module>undertow</module>
    </modules>

    <profiles>
        <profile>
            <id>community</id>
            <activation>
                <property>
                    <name>!product</name>
                </property>
            </activation>
            <modules>
                <module>jetty94</module>
                <module>tomcat8</module>
            </modules>
        </profile>
    </profiles>
</project>
